{
  "name": "messaging",
  "version": "1.0.0",
  "description": "Asynchronous message bus based on websockets.",
  "main": "index.js",
  "watch": {
    "test": {
      "patterns": "{lib,spec}/*.js",
      "quiet": true
    },
    "lint": {
      "patterns": [
        "index.js",
        "{lib,spec}/*.js"
      ],

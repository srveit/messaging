  "keywords": [
    "asynchrous",
    "messaging"
  ],
  "author": "Stephen R. Veit",
  "repository": {
    "type": "git",
    "url": "git://github.com/srveit/messaging.git"
  },
  "license": "ISC",
  "dependencies": {
    "eslint": "^4.19.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "eslint-plugin-jasmine": "^2.9.3",
    "jasmine": "^3.1.0",
    "npm-watch": "^0.3.0"
  }
}

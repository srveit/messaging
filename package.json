{
  "name": "messaging",
  "version": "1.0.1",
  "description": "Asynchronous message bus based on websockets.",
  "keywords": [
    "asynchrous",
    "messaging",
    "websockets"
  ],
  "homepage": "https://github.com/srveit/messaging#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/srveit/messaging.git"
  },
  "bugs": {
